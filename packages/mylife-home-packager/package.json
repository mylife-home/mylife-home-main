{
  "name": "mylife-home-packager",
  "version": "1.0.1",
  "description": "MyLife Home Packager",
  "author": "Vincent Trumpff",
  "homepage": "https://github.com/mylife-home/mylife-home-main#readme",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mylife-home/mylife-home-main.git"
  },
  "scripts": {
    "build-all:prod": "gulp build:prod:ui && gulp build:prod:core && gulp build:prod:studio",
    "build-all:dev": "gulp build:dev:ui && gulp build:dev:core && gulp build:dev:studio",
    "start:serve-dist": "http-server -c-1 ./dist",
    "start:core": "node --trace-warnings dist/dev/core/bin.js --config=test/core/config.json | ../mylife-home-common/node_modules/.bin/bunyan",
    "start:ui": "node --trace-warnings dist/dev/ui/bin.js --config=test/ui/config.json | ../mylife-home-common/node_modules/.bin/bunyan",
    "start:studio": "node --trace-warnings dist/dev/studio/bin.js --config=test/studio/config.json | ../mylife-home-common/node_modules/.bin/bunyan",
    "start:collector": "node --trace-warnings dist/dev/collector/bin.js --config=test/collector/config.json | ../mylife-home-common/node_modules/.bin/bunyan",
    "docker:start:ui": "docker run --rm -d -p 8001:80 -e BUS_SERVER=tcp://rpi-dev-home-main -e DATA_PATH=/tmp --name mylife-home-ui mylife-home-ui:testing",
    "docker:stop:ui": "docker rm -f mylife-home-ui",
    "docker:restart:ui": "npm run docker:stop:ui && npm run docker:start:ui",
    "docker:start:core": "docker run --rm -d -e BUS_SERVER=tcp://rpi-dev-home-main --name mylife-home-core mylife-home-core:testing",
    "docker:stop:core": "docker rm -f mylife-home-core",
    "docker:restart:core": "npm run docker:stop:core && npm run docker:start:core",
    "docker:start:studio": "docker run --rm -d -p 8002:80 -e BUS_SERVER=tcp://rpi-dev-home-main -v $(realpath ../../../mylife-home-studio-data-dev):/data --name mylife-home-studio mylife-home-studio:testing",
    "docker:stop:studio": "docker rm -f mylife-home-studio",
    "docker:restart:studio": "npm run docker:stop:studio && npm run docker:start:studio",
    "docker:start:collector": "docker run --rm -d -e BUS_SERVER=tcp://rpi-dev-home-main.mti-team2.dyndns.org -e MONGO_URL=mongodb://mongo/mylife-home --net=mongo_default --name mylife-home-collector mylife-home-collector:testing",
    "docker:stop:collector": "docker rm -f mylife-home-collector",
    "docker:restart:collector": "npm run docker:stop:collector && npm run docker:start:collector"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/fancy-log": "^1.3.1",
    "@types/gulp": "^4.0.9",
    "@types/merge2": "^1.3.0",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/rimraf": "^3.0.0",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-merge": "^5.0.0",
    "@types/yargs": "^17.0.10",
    "babel-loader": "^8.2.5",
    "before-build-webpack": "^0.2.12",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.5.1",
    "del": "^6.0.0",
    "fancy-log": "^2.0.0",
    "file-loader": "^6.2.0",
    "glob": "^8.0.1",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "merge2": "^1.4.1",
    "mini-css-extract-plugin": "^2.5.1",
    "node-loader": "^2.0.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^12.6.0",
    "shebang-loader": "0.0.1",
    "source-map-loader": "^4.0.1",
    "source-map-support": "^0.5.19",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.5",
    "version-exists": "^0.0.4",
    "webpack": "^5.23.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "yargs": "^17.0.10"
  },
  "dependencies": {
    "mylife-home-collector": "^1.0.1",
    "mylife-home-common": "^1.0.6",
    "mylife-home-core": "^1.0.20",
    "mylife-home-core-plugins-driver-absoluta": "^1.0.8",
    "mylife-home-core-plugins-driver-broadlink": "^1.0.3",
    "mylife-home-core-plugins-driver-lirc": "^1.0.2",
    "mylife-home-core-plugins-driver-mpd": "^1.0.3",
    "mylife-home-core-plugins-driver-sensors": "^1.0.2",
    "mylife-home-core-plugins-driver-sysfs": "^1.0.6",
    "mylife-home-core-plugins-driver-tahoma": "^1.0.8",
    "mylife-home-core-plugins-logic-base": "^1.0.11",
    "mylife-home-core-plugins-logic-clim": "^1.0.2",
    "mylife-home-core-plugins-logic-colors": "^1.0.3",
    "mylife-home-core-plugins-logic-selectors": "^1.0.3",
    "mylife-home-core-plugins-logic-timers": "^1.0.9",
    "mylife-home-core-plugins-ui-base": "^1.0.5",
    "mylife-home-studio": "^1.0.56",
    "mylife-home-ui": "^1.0.9"
  }
}
