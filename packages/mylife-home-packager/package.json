{
  "name": "mylife-home-packager",
  "version": "1.0.0",
  "description": "MyLife Home Packager",
  "author": "Vincent Trumpff",
  "homepage": "https://github.com/mylife-home/mylife-home-main#readme",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mylife-home/mylife-home-main.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:prod": "webpack --config=webpack.config/cli.ts --env.mode=prod --env.wait",
    "build:dev": "webpack --config=webpack.config/cli.ts --env.mode=dev --env.wait",
    "start:serve-dist": "http-server -c-1 ./dist",
    "start:irc-bridge": "node dist/dev/core/bin.js --config=test/irc-bridge/config.json | ../mylife-home-common/node_modules/.bin/bunyan",
    "start:ui": "node dist/dev/ui/bin.js --config=test/ui/config.json | ../mylife-home-common/node_modules/.bin/bunyan"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/fancy-log": "^1.3.1",
    "@types/gulp": "^4.0.6",
    "@types/merge2": "^1.3.0",
    "@types/terser-webpack-plugin": "^3.0.0",
    "@types/webpack": "^4.41.18",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-merge": "^4.1.5",
    "@types/yargs": "^15.0.5",
    "babel-loader": "^8.1.0",
    "before-build-webpack": "^0.2.9",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "del": "^5.1.0",
    "fancy-log": "^1.3.3",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "less": "^3.11.3",
    "less-loader": "^6.1.2",
    "merge2": "^1.4.1",
    "shebang-loader": "0.0.1",
    "source-map-loader": "^1.0.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^5.0.0-beta.17",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "yargs": "^15.4.0"
  },
  "dependencies": {
    "mylife-home-common": "^1.0.0",
    "mylife-home-core": "^1.0.0",
    "mylife-home-core-plugins-irc": "^1.0.0",
    "mylife-home-ui": "^1.0.0"
  }
}
